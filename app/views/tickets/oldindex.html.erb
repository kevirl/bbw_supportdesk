<table>
  <thead>
    <tr>
      <th>Ttype</th>
      <th>Email</th>
      <th>Status</th>
      <th>Title</th>
      <th>Caselog</th>
      <th>Category</th>
      <th>Product</th>
      <th>Solution</th>
      <th>Account</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.ttype %></td>
        <td><%= ticket.email %></td>
        <td><%= ticket.status %></td>
        <td><%= ticket.title %></td>
        <td><%= ticket.caselog %></td>
        <td><%= ticket.category %></td>
        <td><%= ticket.product %></td>
        <td><%= ticket.solution %></td>
        <td><%= ticket.account %></td>
        <td><%= link_to 'Show', ticket %></td>
        <td><%= link_to 'Edit', edit_ticket_path(ticket) %></td>
        <td><%= link_to 'Destroy', ticket, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket', new_ticket_path %>
